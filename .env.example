# Configuração do Oliva Church App

# URL da aplicação web de produção
OLIVA_BASE_URL=https://oliva.church/

# Ativar modo debug (console logs)
DEBUG_MODE=false

# Domínio permitido (não altere)
OLIVA_DOMAIN=oliva.church

# User-Agent customizado (identifica requisições do app)
USER_AGENT=OlivaChurchApp/1.0

# Configurações de cache
CACHE_MAX_AGE_MS=3600000

# Timeout para requisições (ms)
REQUEST_TIMEOUT=30000
